{
  "version": "1.0",
  "defaultProvider": "unpkg",
    "libraries": [
        {
            "library": "@aspnet/signalr@1.0.4",
            "destination": "wwwroot/lib/signalr/",
            "files": [
                "dist/browser/signalr.min.js"
            ]
        },
        {
            "library": "jquery@3.3.1",
            "destination": "wwwroot/lib/jquery/",
            "files": [
                "dist/jquery.min.js"
            ]
        },
        {
            "provider": "cdnjs",
            "library": "crypto-js@3.1.9-1",
            "destination": "wwwroot/lib/crypto-js/",
            "files": [
                "crypto-js.min.js"
            ]
        },
        {
            "provider": "cdnjs",
            "library": "MaterialDesign-Webfont@2.8.94",
            "destination": "wwwroot/lib/MaterialDesign-Webfont/",
            "files": [
                "css/materialdesignicons.min.css"
            ]
        },
        {
            "provider": "cdnjs",
            "library": "modernizr@2.8.3",
            "destination": "wwwroot/lib/modernizr/"
        },
        {
            "provider": "cdnjs",
            "library": "moment.js@2.22.2",
            "destination": "wwwroot/lib/momentjs/",
            "files": [
                "moment.min.js"
            ]
        }
    ]
}